<section id="resortContainer">
    <!-- this is a loop with handlebars -->
    {{#each resort}}
        <div class="thumbnail">
            <a href="/{{name}}">
                <img src="/images/{{image}}" alt="image">
            </a>

            <h3 class="hero-name">{{name}}</h3>
        </div>        
    {{/each}}
</section>

